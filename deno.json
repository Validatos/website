{
  "lock": false,
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check **/*.ts && deno check **/*.tsx",
    "start": "deno run -A --watch=static/,routes/ dev.ts",
    "dev": "deno run -A --watch=static/,routes/ dev.ts",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A main.ts",
    "update": "deno run -A -r https://fresh.deno.dev/update ."
  },
  "lint": {
    "rules": {
      "tags": ["fresh", "recommended"]
    }
  },
  "exclude": ["**/_fresh/*"],
  "imports": {
    "#/": "./",
    "$fresh/": "https://deno.land/x/fresh@1.5.2/",
    "$std/": "https://deno.land/std@0.193.0/",
    "$tabler_icons/": "https://deno.land/x/tabler_icons_tsx@0.0.4/",
    "@openapi-explorer": "npm:/openapi-explorer@1.1.578",
    "preact": "npm:/preact@10.18.1",
    "preact/": "npm:/preact@10.18.1/",
    "preact-render-to-string": "npm:/preact-render-to-string@6.2.2",
    "@preact/signals": "npm:/@preact/signals@1.2.1",
    "@preact/signals-core": "npm:/@preact/signals-core@1.5.0",
    "@twind/core": "npm:/@twind/core@1.1.3",
    "@twind/preset-autoprefix": "npm:/@twind/preset-autoprefix@1.0.7",
    "@twind/preset-tailwind/": "npm:/@twind/preset-tailwind@1.1.4/"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
